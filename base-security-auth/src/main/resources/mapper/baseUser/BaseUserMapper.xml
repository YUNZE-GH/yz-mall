<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yz.auth.business.baseUser.mapper.BaseUserMapper">

    <select id="findOneByUserAccount" resultType="com.yz.auth.business.baseUser.entity.BaseUser">
        select * from base_user where user_account = #{userAccount,jdbcType=VARCHAR}
    </select>
</mapper>
